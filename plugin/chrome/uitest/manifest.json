{
    "name": "uitest-chrome extension",
    "version": "2.0",
    "manifest_version": 2,
    "description": "chrome extension for uitest ",
    "page_action":
    {
        "default_icon": "1.jpg",
        "default_title": "uitest"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["addscript.js"],
            "all_frames": true
        }
    ],
    "icons": {
        "48": "1.jpg"
    }
}  